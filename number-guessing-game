import random
print("Welcome to the Number Guessing Game")
print("Guess the number between 0 to 10")
def match(a, c):
  if a == c:
    return 0
  if a != c:
    return 1

a = int(input("Enter a number: "))
c = random.randint(0, 10)
# print("Random Number is", c)

score = match(a, c)
if (score == 0):
  print("You won")
elif (score == 1):
  print("You lost")

print("Random Number was", c)

